package blorb;

import java.util.Random;

@javax.jws.WebService
public interface eshopIfce {

	public class order{
	
		int count;
		
		public order(int c) {
			this.count = c;
		}
		
		@javax.jws.WebMethod
		public boolean pay() {
			return new Random().nextBoolean();
		}
		
	}
	
	@javax.jws.WebMethod
	public order orderYoghurt(int count);
	
}
